#include <stdio.h>
int main()
{
    int A[30],n,x,i,flag=0;
    printf("Enter the size of array\n");
    scanf("%d",&n);
    printf("Enter the elements into array\n");
    for ( i = 0; i < n; i++)
    {
       scanf("%d",&A[i]);
    }
    printf("Enter the element to be searched\n");
    scanf("%d",&x);

    for ( i = 0; i < n; i++)
    {
       if (A[i]==x)
       {
          flag=1;
          break;
       }
       
    }
    if (flag==0)
    
       printf("Element not found");
    
    else
   
       printf("Element fount at index %d",i);
   
return 0;
}

